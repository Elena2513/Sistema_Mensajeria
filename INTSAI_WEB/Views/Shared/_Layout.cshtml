    <script type="text/javascript">

        $(".btn-print").click(function () {
            if ($(this).attr("data-print") !== undefined) {
                let printContent = $(this).attr("data-print");
                $(".btn-aceptar").hide();
                $(".btn-rechazar").hide();
                $("#" + printContent).printThis();
            }
            else {
                $(".btn-option").hide();
                $("#print-document").printThis({

                    header: "<h1 class='text-center'>NOMBRE INSTITUCION</h1><h3 class='text-center'>NameEmpresa</h3><h4 class='text-center' style='text-decoration:underline;'>" + delegacion + "</h4>",
                    footer: "<p>Impreso en la fecha @DateTime.Now.ToShortDateString()</p>",

                });
            }
        });

    </script>

